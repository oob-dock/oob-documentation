{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$defs": {
        "ResponsePersonalAccounts": {
            "type": "object",
            "required": [
                "brand"
            ],
            "properties": {
                "brand": {
                    "$ref": "#/$defs/PersonalAccountBrand"
                }
            },
            "additionalProperties": false
        },
        "PersonalAccountBrand": {
            "type": "object",
            "required": [
                "name",
                "companies"
            ],
            "properties": {
                "name": {
                    "$ref": "../../../common/common-schema.json#/$defs/Name",
                    "description": "Nome da Marca reportada pelo participante do Open Banking. O conceito a que se refere a 'marca' é em essência uma promessa da empresa em fornecer uma série específica de atributos, benefícios e serviços uniformes aos clientes."
                },
                "companies": {
                    "$ref": "#/$defs/PersonalAccountCompany"
                }
            },
            "additionalProperties": false
        },
        "PersonalAccountCompany": {
            "type": "array",
            "minItems": 1,
            "description": "Companies traz uma lista de todas as instituições da Marca.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "name",
                            "cnpjNumber",
                            "personalAccounts"
                        ],
                        "properties": {
                            "name": {
                                "$ref": "../../../common/common-schema.json#/$defs/Name",
                                "description": "Nome da Instituição, pertencente à marca, responsável pela modalidade de Empréstimos. p.ex.'Empresa da Organização A'"
                            },
                            "cnpjNumber": {
                                "$ref": "../../../common/common-schema.json#/$defs/CnpjNumber"
                            },
                            "urlComplementaryList": {
                                "$ref": "../../../common/common-schema.json#/$defs/urlComplementaryList"
                            },
                            "personalAccounts": {
                                "$ref": "#/$defs/PersonalAccounts"
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "PersonalAccounts": {
            "type": "array",
            "minItems": 1,
            "maxItems": 3,
            "description": "Lista de tipos de conta.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "type",
                            "fees",
                            "serviceBundles",
                            "openingClosingChannels",
                            "transactionMethods",
                            "termsConditions"
                        ],
                        "properties": {
                            "type": {
                                "$ref": "#/$defs/AccountType"
                            },
                            "fees": {
                                "$ref": "#/$defs/AccountFee"
                            },
                            "serviceBundles": {
                                "$ref": "#/$defs/ServiceBundle"
                            },
                            "openingClosingChannels": {
                                "$ref": "#/$defs/OpeningClosingChannels"
                            },
                            "additionalInfo": {
                                "$ref": "#/$defs/AdditionalInfo"
                            },
                            "transactionMethods": {
                                "$ref": "#/$defs/TransactionMethods"
                            },
                            "termsConditions": {
                                "$ref": "#/$defs/AccountsTermsConditions"
                            },
                            "incomeRate": {
                                "$ref": "#/$defs/AccountsIncomeRate"
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        
        "AccountType": {
            "type": "string",
            "description": "Tipos de contas ofertadas para pessoa natural ou jurídica, p.ex. 'CONTA_DEPOSITO_A_VISTA'. Conta de depósito à vista ou Conta corrente - é o tipo mais comum. Nela, o dinheiro fica à sua disposição para ser sacado a qualquer momento. Essa conta não gera rendimentos para o depositante Conta poupança - foi criada para estimular as pessoas a pouparem. O dinheiro que ficar na conta por trinta dias passa a gerar rendimentos, com isenção de imposto de renda para quem declara. Ou seja, o dinheiro “cresce” (rende) enquanto ficar guardado na conta. Cada depósito terá rendimentos de mês em mês, sempre no dia do mês em que o dinheiro tiver sido depositado Conta de pagamento pré-paga: segundo CIRCULAR Nº 3.680, BCB de 2013, é a 'destinada à execução de transações de pagamento em moeda eletrônica realizadas com base em fundos denominados em reais previamente aportados'.",
            "enum": [
                "CONTA_DEPOSITO_A_VISTA", 
                "CONTA_POUPANCA", 
                "CONTA_PAGAMENTO_PRE_PAGA"
            ],
            "examples": [
                "CONTA_DEPOSITO_A_VISTA"
            ]
        },
        "AccountFee": {
            "type": "object",
            "description": "Objeto que reúne informações de tarifas de serviços.",
            "required": [
                "priorityServices",
                "otherServices"
            ],
            "properties": {
                "priorityServices": {
                    "$ref": "#/$defs/AccountPriorityService"
                },
                "otherServices": {
                    "$ref": "#/$defs/AccountOtherService"
                }
            },
            "additionalProperties": false
        },
        "AccountPriorityService": {
            "type": "array",
            "minItems": 1,
            "maxItems": 40,
            "description": "Lista das Tarifas cobradas sobre Serviços Prioritários.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "name",
                            "code",
                            "chargingTriggerInfo",
                            "prices",
                            "minimum",
                            "maximum"
                        ],
                        "properties": {
                            "name": {
                                "$ref": "#/$defs/PriorityServiceName"
                            },
                            "code": {
                                "$ref": "#/$defs/AccountPriorityServiceCode"
                            },
                            "chargingTriggerInfo": {
                                "$ref": "../../../common/common-schema.json#/$defs/ChargingTriggerInfo",
                                "description": "Fatos geradores de cobrança que incidem sobre os serviços prioritários, segundo Resolução 3.919 do Bacen, para pessoa natural."
                            },
                            "prices": {
                                "$ref": "../../../common/common-schema.json#/$defs/Price"
                            },
                            "minimum": {
                                "$ref": "../../../common/common-schema.json#/$defs/MinimumPrice"
                            },
                            "maximum": {
                                "$ref": "../../../common/common-schema.json#/$defs/MaximumPrice"
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "AccountOtherService": {
            "type": "array",
            "minItems": 1,
            "description": "Lista das Tarifas cobradas sobre outros Serviços, que não prioritários.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "name",
                            "code",
                            "chargingTriggerInfo",
                            "prices",
                            "minimum",
                            "maximum"
                        ],
                        "properties": {
                            "name": {
                                "$ref": "../../../common/common-schema.json#/$defs/ServiceName",
                                "description": "An explanation about the purpose of this instance."
                            },
                            "code": {
                                "$ref": "../../../common/common-schema.json#/$defs/Code",
                                "description": "Siglas de identificação do Serviço (Campo Livre)."
                            },
                            "chargingTriggerInfo": {
                                "$ref": "../../../common/common-schema.json#/$defs/ChargingTriggerInfo",
                                "description": "Fatos geradores de cobrança que incidem sobre outros serviços para pessoa natural. (Campo Livre)."
                            },
                            "prices": {
                                "$ref": "../../../common/common-schema.json#/$defs/Price"
                            },
                            "minimum": {
                                "$ref": "../../../common/common-schema.json#/$defs/MinimumPrice"
                            },
                            "maximum": {
                                "$ref": "../../../common/common-schema.json#/$defs/MaximumPrice"
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "ResponseBusinessAccounts": {
            "type": "object",
            "required": [
                "brand"
            ],
            "properties": {
                "brand": {
                    "$ref": "#/$defs/BusinessAccountBrand"
                }
            },
            "additionalProperties": false
            
        },
        "BusinessAccountBrand": {
            "type": "object",
            "required": [
                "name",
                "companies"
            ],
            "properties": {
                "name": {
                    "$ref": "../../../common/common-schema.json#/$defs/Name",
                    "description": "Nome da Marca reportada pelo participante do Open Banking. O conceito a que se refere a 'marca' é em essência uma promessa da empresa em fornecer uma série específica de atributos, benefícios e serviços uniformes aos clientes."
                },
                "companies": {
                    "$ref": "#/$defs/BusinessAccountCompany"
                }
            },
            "additionalProperties": false
        },
        "BusinessAccountCompany": {
            "type": "array",
            "minItems": 1,
            "description": "Companies traz uma lista de todas as instituições da Marca.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "name",
                            "cnpjNumber",
                            "businessAccounts"
                        ],
                        "properties": {
                            "name": {
                                "$ref": "../../../common/common-schema.json#/$defs/Name",
                                "description": "Nome da Instituição, pertencente à marca, responsável pela modalidade de Empréstimos. p.ex.'Empresa da Organização A'"
                            },
                            "cnpjNumber": {
                                "$ref": "../../../common/common-schema.json#/$defs/CnpjNumber"
                            },
                            "urlComplementaryList": {
                                "$ref": "../../../common/common-schema.json#/$defs/urlComplementaryList"
                            },
                            "businessAccounts": {
                                "$ref": "#/$defs/BusinessAccounts"
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "BusinessAccounts": {
            "type": "array",
            "minItems": 1,
            "maxItems": 3,
            "description": "Lista de tipos de conta.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "type",
                            "fees",
                            "serviceBundles",
                            "openingClosingChannels",
                            "transactionMethods",
                            "termsConditions"
                        ],
                        "properties": {
                            "type": {
                                "$ref": "#/$defs/AccountType"
                            },
                            "fees": {
                                "$ref": "#/$defs/Fee"
                            },
                            "serviceBundles": {
                                "$ref": "#/$defs/ServiceBundle"
                            },
                            "openingClosingChannels": {
                                "$ref": "#/$defs/OpeningClosingChannels"
                            },
                            "additionalInfo": {
                                "$ref": "#/$defs/AdditionalInfo"
                            },
                            "transactionMethods": {
                                "$ref": "#/$defs/TransactionMethods"
                            },
                            "termsConditions": {
                                "$ref": "#/$defs/AccountsTermsConditions"
                            },
                            "incomeRate": {
                                "$ref": "#/$defs/AccountsIncomeRate"
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "Fee": {
            "type": "object",
            "description": "Objeto que reúne informações de tarifas de serviços.",
            "required": [
                "services"
            ],
            "properties": {
                "services": {
                    "$ref": "#/$defs/BusinessAccountsService"
                }
            },
            "additionalProperties": false
        },
        "BusinessAccountsService": {
            "type": "array",
            "description": "Lista das Tarifas cobradas sobre Serviços.",
            "additionalItems": true,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "name",
                            "code",
                            "chargingTriggerInfo",
                            "prices",
                            "minimum",
                            "maximum"
                        ],
                        "properties": {
                            "name": {
                                "$ref": "../../../common/common-schema.json#/$defs/ServiceName",
                                "description": "Nome do Serviço que incide sobre tipo de conta selecionado para pessoa jurídica(Campo Livre)."
                            },
                            "code": {
                                "$ref": "../../../common/common-schema.json#/$defs/Code",
                                "description": "Sigla de identificação de Outros Serviços que incidem sobre os tipos de contas informados."
                            },
                            "chargingTriggerInfo": {
                                "$ref": "../../../common/common-schema.json#/$defs/ChargingTriggerInfo",
                                "description": "Fatos geradores de cobrança que incidem sobre serviço que compõe o Pacote de Serviços."
                            },
                            "prices": {
                                "$ref": "../../../common/common-schema.json#/$defs/Price"
                            },
                            "minimum": {
                                "$ref": "../../../common/common-schema.json#/$defs/MinimumPrice"
                            },
                            "maximum": {
                                "$ref": "../../../common/common-schema.json#/$defs/MaximumPrice"
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "ServiceBundle": {
            "type": "array",
            "minItems": 1,
            "description": "Lista dos Pacotes de serviços.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "name",
                            "services",
                            "prices",
                            "minimum",
                            "maximum"
                        ],
                        "properties": {
                            "name": {
                                "$ref": "../../../common/common-schema.json#/$defs/ServiceName",
                                "description": "Nome do Pacote de Serviços dado pela instituição."
                            },
                            "services": {
                                "$ref": "#/$defs/ServiceBundleServiceDetail"
                            },
                            "prices": {
                                "$ref": "../../../common/common-schema.json#/$defs/Price"
                            },
                            "minimum": {
                                "$ref": "../../../common/common-schema.json#/$defs/MinimumPrice"
                            },
                            "maximum": {
                                "$ref": "../../../common/common-schema.json#/$defs/MaximumPrice"
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "OpeningClosingChannels": {
            "type": "array",
            "minItems": 1,
            "maxItems": 7,
            "description": "Lista dos canais para aberturas e encerramento.",
            "additionalItems": false,
            "Enum": [ 
                "DEPENDENCIAS_PROPRIAS", 
                "CORRESPONDENTES_BANCARIOS", 
                "INTERNET_BANKING", 
                "MOBILE_BANKING", 
                "CENTRAL_TELEFONICA", 
                "CHAT", 
                "OUTROS" 
            ],
            "items": {
                "anyOf": [
                    {
                        "type": "string",
                        "examples": [
                            "DEPENDENCIAS_PROPRIAS",
                            "CORRESPONDENTES_BANCARIOS"
                        ]
                    }
                ]
            }
        },
        "AdditionalInfo": {
            "type": "string",
            "maxLength": 100,
            "pattern": "\\w*\\W*",
            "description": "Texto livre para complementar informação relativa ao Canal disponível, quando no campo ''openingClosingChannels'' estiver preenchida a opção ''Outros'' Restrição: Campo de preenchimento obrigatório se ''openingCloseChannels'' estiver preenchida a opção ''OUTROS''.",
            "examples": [
                "WhatsApp"
            ]
        },
        "TransactionMethods": {
            "type": "array",
            "minItems": 1,
            "maxItems": 4,
            "description": "Lista de formas de movimentação possíveis para a conta.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "string",
                        "Enum": [ 
                            "MOVIMENTACAO_ELETRONICA", 
                            "MOVIMENTACAO_CHEQUE", 
                            "MOVIMENTACAO_CARTAO", 
                            "MOVIMENTACAO_PRESENCIAL" 
                        ],
                        "examples": [
                            "MOVIMENTACAO_CARTAO"
                        ]
                    }
                ]
            }
        },
        "AccountsTermsConditions": {
            "type": "object",
            "description": "Objeto que reúne informações relativas a Termos e Condições para as modalidades tratadas.",
            "required": [
                "minimumBalance",
                "elegibilityCriteriaInfo",
                "closingProcessInfo"
            ],
            "properties": {
                "minimumBalance": {
                    "$ref": "#/$defs/MinimumBalance"
                },
                "elegibilityCriteriaInfo": {
                    "type": "string",
                    "maxLength": 2000,
                    "pattern": "\\w*\\W*",
                    "description": "Critérios de qualificação do cliente com a finalidade de definir sua elegibilidade para a aquisição do tipo de conta. Campo Aberto.",
                    "examples": [
                        "https://empresaa1.com/Accounts_closingProcess"
                    ]
                },
                "closingProcessInfo": {
                    "type": "string",
                    "maxLength": 2000,
                    "pattern": "\\w*\\W*",
                    "description": "Procedimentos de encerramento para o tipo de conta tratado. Possibilidade de inscrição da URL. Endereço eletrônico de acesso ao canal. p.ex. 'https://example.com/mobile-banking'.",
                    "examples": [
                        "https://empresaa1.com/Accounts_closingProcess"
                    ]
                }
            },
            "additionalProperties": false
        },
        "MinimumBalance": {
            "type": "object",
            "required": [
                "value",
                "currency"
            ],
            "properties": {
                "value": {
                    "$ref": "../../../common/common-schema.json#/$defs/Value",
                    "description": "Saldo mínimo exigido nos Termos e condições contratuais, que regem as contas comercializadas."
                },
                "currency": {
                    "$ref": "../../../common/common-schema.json#/$defs/Currency"
                }
            },
            "additionalProperties": false
        },
        "AccountsIncomeRate": {
            "type": "array",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "properties": {
                            "savingAccount": {
                                "type": "string",
                                "maxLength": 2000,
                                "pattern": "\\w*\\W*",
                                "minItems": 1,
                                "maxItems": 1,
                                "description": "Descrição da Remuneração especificamente para Conta de Poupança. Deve ser preenchido com a determinação legal vigente. Restrição: De preenchimento obrigatório para CONTA_POUPANCA.",
                                "examples": [
                                    "Para depósitos até 03/05/2012 – remuneração trimestral de 1,5% + TR, sempre creditada no aniversário da conta; Para depósitos a partir de 04/05/2012 – 70% da Selic + TR quando a Selic for igual ou inferior a 8,5% ao ano e 1,5% + TR caso a Selic seja superior a 8,5%."
                                ]
                            },
                            "prepaidPaymentAccount": {
                                "type": "string",
                                "maxLength": 2000,
                                "pattern": "\\w*\\W*",
                                "minItems": 1,
                                "maxItems": 1,
                                "description": "Campo Livre. Deve explicitar o Percentual em favor do titular da conta de pagamento pré-paga.",
                                "examples": [
                                    "NA"
                                ]
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "ServiceBundleServiceDetail": {
            "type": "array",
            "minItems": 1,
            "description": "Lista dos serviços que compõe o pacote de serviços.",
            "additionalItems": false,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "code",
                            "chargingTriggerInfo",
                            "eventLimitQuantity",
                            "freeEventQuantity"
                        ],
                        "properties": {
                            "code": {
                                "$ref": "../../../common/common-schema.json#/$defs/Code",
                                "description": "Código que identifica o Serviço que compõe o Pacote de Serviços, podendo ser da lista de Serviços Prioritários ou Outros Serviços. p.ex. segundo Resolução 3.919 do Bacen: 'SAQUE_TERMINAL'."
                            },
                            "chargingTriggerInfo": {
                                "$ref": "../../../common/common-schema.json#/$defs/ChargingTriggerInfo",
                                "description": "Fatos geradores de cobrança que incidem sobre serviço que compõe o Pacote de Serviços."
                            },
                            "eventLimitQuantity": {
                                "type": "string",
                                "maxLength": 6,
                                "pattern": "^(\\d{1,6}){1}$",
                                "description": "Segundo Resolução 4196, BCB, de 2013: Quantidade de eventos previstos no Pacote de Serviços (Número de eventos incluídos no mês) p.ex.'2'. No caso de quantidade ilimitada, reportar 999999.",
                                "examples": [
                                    "2"
                                ]
                            },
                            "freeEventQuantity": {
                                "type": "string",
                                "maxLength": 6,
                                "pattern": "^(\\d{1,6}){1}$",
                                "description": "Segundo Resolução 4196, BCB, de 2013: Quantidade de eventos previstos no Pacote de Serviços com isenção de Tarifa.p.ex.'1' No caso de quantidade ilimitada, reportar 999999.",
                                "examples": [
                                    "0"
                                ]
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "PriorityServiceName": {
            "type": "string",
            "description": "Nome dos Serviços prioritários, segundo Resolução 3.919 do Bacen, para pessoa natural.",
            "examples": [
                "TRANSFERENCIA_TED_PESSOAL_OU_PRESENCIAL"
            ],
            "Enum": [ 
                "CONFECCAO_CADASTRO_INICIO_RELACIONAMENTO", 
                "FORNECIMENTO_2_VIA_CARTAO_FUNCAO_DEBITO", 
                "FORNECIMENTO_2_VIA_CARTAO_FUNCAO_MOVIMENTACAO_CONTA_POUPANCA", 
                "EXCLUSAO_CADASTRO_EMITENTES_CHEQUES_SEM_FUNDO_CCF", 
                "CONTRA_ORDEM_REVOGACAO_E_OPOSICAO_OU_SUSTACAO_PAGAMENTO_CHEQUE", 
                "FORNECIMENTO_FOLHAS_CHEQUE", 
                "CHEQUE_ADMINISTRATIVO", 
                "CHEQUE_VISADO", 
                "SAQUE_CONTA_DEPOSITO_A_VISTA_POUPANCA_PRESENCIAL_OU_PESSOAL", 
                "SAQUE_CONTA_DEPOSITO_A_VISTA_POUPANCA_TERMINAL_AUTOATENDIMENTO", 
                "SAQUE_CONTA_DEPOSITO_A_VISTA_POUPANCA_CORRESPONDENTES_PAIS", 
                "DEPOSITO_IDENTIFICADO", 
                "FORNECIMENTO_EXTRATO_MENSAL_CONTA_DEPOSITOS_A_VISTA_E_POUPANCA_PRESENCIAL_OU_PESSOAL", 
                "FORNECIMENTO_EXTRATO_MENSAL_CONTA_DEPOSITOS_A_VISTA_E_POUPANCA_TERMINAL_AUTOATENDIMENTO", 
                "FORNECIMENTO_EXTRATO_MENSAL_CONTA_DEPOSITOS_A_VISTA_E_POUPANCA_CORRESPONDENTES_PAIS", 
                "FORNECIMENTO_EXTRATO_DE_UM_PERIODO_CONTA_DEPOSITOS_A_VISTA_E_POUPANCA_PRESENCIAL_OU_PESSOAL", 
                "FORNECIMENTO_EXTRATO_DE_UM_PERIODO_CONTA_DEPOSITOS_A_VISTA_E_POUPANCA_TERMINAL_AUTOATENDIMENTO", 
                "FORNECIMENTO_EXTRATO_DE_UM_PERIODO_CONTA_DEPOSITOS_A_VISTA_E_POUPANCA_CORRESPONDENTES_PAIS", 
                "FORNECIMENTO_COPIA_MICROFILME_MICROFICHA_ASSEMELHADO", 
                "TRANSFERENCIA_DOC_PESSOAL_OU_PRESENCIAL", 
                "TRANSFERENCIA_DOC_TERMINAL_AUTOATENDIMENTO_OUTROS_MEIOS_ELETRONICOS", 
                "TRANSFERENCIA_DOC_INTERNET", 
                "TRANSFERENCIA_TED_PESSOAL_OU_PRESENCIAL", 
                "TRANSFERENCIA_TED_TERMINAL_AUTOATENDIMENTO_OUTROS_MEIOS_ELETRONICOS", 
                "TRANSFERENCIA_TED_INTERNET", 
                "TRANSFERENCIA_DOC_TED_PESSOAL_OU_PRESENCIAL", 
                "TRANSFERENCIA_DOC_TED_TERMINAL_AUTOATENDIMENTO_OUTROS_MEIOS_ELETRONICOS", 
                "TRANSFERENCIA_DOC_TED_INTERNET", 
